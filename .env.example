# Relworx API credentials
RELWORX_API_KEY=your_relworx_api_key
RELWORX_ACCOUNT_NO=your_relworx_account_no
RELWORX_BASE_URL=https://payments.relworx.com/api

# Development environment
NODE_ENV=development

# Optional: Control logging and debugging
DEBUG_LOGS=true
SUPPRESS_STARTUP_LOGS=false

# SQLite database path
SQLITE_DB_PATH=./payments.db

# Backend API key for protecting endpoints
BACKEND_API_KEY=your_backend_api_key

# CORS configuration
ALLOWED_ORIGIN=http://localhost:5173

# Server port
PORT=3000

# Supabase configuration
SUPABASE_URL=your-project-ref.supabase.co
SUPABASE_KEY=your-anon-key